#include <iostream>
#include <vector>
using namespace std;

int main()
{
    long long t;
    cin >> t;

    while(t--)
    {
        long long n;
        cin >> n;

        vector<long long> a(n);
        vector<long long> b(n, 0);

        for(long long i = 0; i < n; i++)
        {
            cin >> a[i];
        }

        long long abc = 0;

        for(long long i = 0; i < n; i++)
        {
            long long xyz = -1;
            long long pos = -1;

            for(long long j = 0; j < n; j++)
            {
                if(b[j] == 0)
                {
                    long long val = abc | a[j];
                    if(val > xyz)
                    {
                        xyz = val;
                        pos = j;
                    }
                }
            }

            if(pos == -1)
            {
                for(long long j = 0; j < n; j++)
                {
                    if(b[j] == 0)
                    {
                        pos = j;
                        break;
                    }
                }
            }

            cout << a[pos] << " ";

            abc = abc | a[pos];
            b[pos] = 1;
        }

        cout << endl;
    }

    return 0;
}
