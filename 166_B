#include <iostream>
#include <vector>
using namespace std;

long long cross(long long x1, long long y1, long long x2, long long y2)
{
    return x1 * y2 - y1 * x2;
}

int main()
{
    long long n;
    cin >> n;

    vector<long long> ax(n), ay(n);

    for(long long i = 0; i < n; i++)
    {
        cin >> ax[i] >> ay[i];
    }

    long long m;
    cin >> m;

    vector<long long> bx(m), by(m);

    for(long long i = 0; i < m; i++)
    {
        cin >> bx[i] >> by[i];
    }

    for(long long i = 0; i < m; i++)
    {
        long long px = bx[i];
        long long py = by[i];

        for(long long j = 0; j < n; j++)
        {
            long long x1 = ax[j];
            long long y1 = ay[j];

            long long x2 = ax[(j + 1) % n];
            long long y2 = ay[(j + 1) % n];

            long long vx1 = x2 - x1;
            long long vy1 = y2 - y1;

            long long vx2 = px - x1;
            long long vy2 = py - y1;

            long long c = cross(vx1, vy1, vx2, vy2);

            if(c >= 0)
            {
                cout << "NO";
                return 0;
            }
        }
    }

    cout << "YES";

    return 0;
}
